{
  "checkers": ["typescript"],
  "disableTypeChecks": true,
  "htmlReporter": { "fileName": "reports/mutation/mutation-etl.html" },
  "ignorePatterns": [".*", "coverage", "dist", "reports", "src/api/e2e.test.ts"],
  "ignoreStatic": true,
  "logLevel": "info",
  "mutate": [
    "src/etl/**/*.ts",
    "src/shared/**/*.ts",
    "!src/**/*.test.ts",
    "!src/api/**/*.ts",
    "!src/etl/clinicalTrialIndexMapping.ts",
    "!src/etl/researchStudyIndexMapping.ts",
    "!src/etl/console.ts",
    "!src/shared/elasticsearch/ElasticsearchConfig.ts",
    "!src/shared/logger/*.ts",
    "!src/shared/test/**/*.ts"
  ],
  "packageManager": "yarn",
  "reporters": ["html"],
  "testRunner": "vitest"
}
