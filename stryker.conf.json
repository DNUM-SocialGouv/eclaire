{
  "checkers": ["typescript"],
  "logLevel": "info",
  "disableTypeChecks": true,
  "ignorePatterns": [".*", "coverage", "dist", "reports", "src/api/e2e.test.ts"],
  "ignoreStatic": true,
  "mutate": [
    "src/**/*.ts",
    "!src/api/auth/*.ts",
    "!src/api/sentry/*.ts",
    "!src/api/swagger/*.ts",
    "!src/api/users/*.ts",
    "!src/etl/clinicalTrialIndexMapping.ts",
    "!src/etl/console.ts",
    "!src/shared/logger/*.ts",
    "!src/shared/test/**/*.ts",
    "!src/**/*.test.ts",
    "!src/**/*Controller.ts",
    "!src/**/*TestingFactory.ts",
    "!src/main.ts",
    "!src/AppModule.ts"
  ],
  "packageManager": "yarn",
  "reporters": ["html"],
  "testRunner": "vitest"
}
