{
  "name": "api-eclaire",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "18.16"
  },
  "packageManager": "yarn@3.5.1",
  "scripts": {
    "amibroken": "yarn dedupe --check && yarn typecheck && yarn lint && yarn test",
    "bash:production": "scalingo -a eclaire-api run bash",
    "build": "nest build",
    "deadcode": "ts-prune --ignore 'vitest.config.ts'",
    "depcheck": "depcheck",
    "dev": "nest start --watch",
    "etl:create-index": "ts-node src/etl/console.ts create-index",
    "etl:import": "ts-node src/etl/console.ts import-riph",
    "lint": "eslint '{src,config}/**/*.ts' --max-warnings=0",
    "start": "node dist/src/main",
    "start:debug": "nest start --debug --watch",
    "start:db": "yarn stop:db && docker-compose up -d",
    "stop:db": "docker-compose down",
    "test": "NODE_ENV=test vitest run",
    "test:coverage": "yarn test --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:mutation": "npx stryker run",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.10.0",
    "@nestjs/common": "^10.0.5",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.5",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.5",
    "@nestjs/swagger": "^7.1.1",
    "@sentry/minimal": "^6.19.7",
    "@sentry/node": "^7.57.0",
    "@sentry/tracing": "^7.57.0",
    "@types/fhir": "^0.0.37",
    "argon2": "^0.30.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "js-yaml": "^4.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.7",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.5",
    "@stryker-mutator/core": "^7.1.0",
    "@stryker-mutator/typescript-checker": "^7.1.0",
    "@stryker-mutator/vitest-runner": "^7.1.0",
    "@swc/core": "^1.3.68",
    "@types/express": "^4.17.17",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "18.11.18",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitest/coverage-istanbul": "^0.29.7",
    "depcheck": "^1.4.3",
    "eslint": "^8.44.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vitest": "^0.2.6",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-prune": "^0.10.3",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.9.5",
    "unplugin-swc": "^1.3.2",
    "vitest": "^0.32.4"
  }
}
