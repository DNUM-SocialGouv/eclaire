{
  "name": "api-eclaire",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "18.16"
  },
  "packageManager": "yarn@3.5.1",
  "scripts": {
    "amibroken": "yarn dedupe --check && yarn typecheck && yarn lint && yarn test",
    "bash:production": "scalingo -a eclaire-api run bash",
    "build": "nest build",
    "deadcode": "ts-prune --ignore 'vitest.config.ts'",
    "depcheck": "depcheck",
    "dev": "nest start --watch",
    "etl:create-index": "ts-node src/etl/console.ts create-index",
    "etl:create-policies": "ts-node src/etl/console.ts create-policies",
    "etl:delete-pipelines": "ts-node src/etl/console.ts delete-pipelines",
    "etl:delete-policies": "ts-node src/etl/console.ts delete-policies",
    "etl:import": "ts-node src/etl/console.ts import",
    "etl:meddra-import": "ts-node src/etl/console.ts meddra-import",
    "etl:reset-index": "ts-node src/etl/console.ts reset-index",
    "etl:translate": "ts-node src/etl/console.ts translate",
    "lint": "eslint 'src/**/*.ts' --max-warnings=0",
    "prepare": "husky install",
    "start": "node dist/src/main",
    "start:db": "yarn stop:db && docker-compose up -d",
    "start:debug": "nest start --debug --watch",
    "stop:db": "docker-compose down",
    "test": "NODE_ENV=test vitest run",
    "test:coverage": "yarn test --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:mutation:api": "npx stryker run stryker-api.conf.json",
    "test:mutation:etl": "npx stryker run stryker-etl.conf.json",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.414.0",
    "@elastic/elasticsearch": "7.10.0",
    "@nestjs/common": "^10.0.5",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.5",
    "@nestjs/platform-express": "^10.0.5",
    "@nestjs/swagger": "^7.1.1",
    "@sentry/minimal": "^6.19.7",
    "@sentry/node": "^7.57.0",
    "@sentry/tracing": "^7.57.0",
    "@types/fhir": "^0.0.37",
    "express": "^4.18.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.7",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.5",
    "@stryker-mutator/core": "^7.1.0",
    "@stryker-mutator/typescript-checker": "^7.1.0",
    "@stryker-mutator/vitest-runner": "^7.1.0",
    "@swc/core": "^1.3.68",
    "@types/express": "^4.17.17",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitest/coverage-istanbul": "^0.29.7",
    "depcheck": "^1.4.3",
    "eslint": "^8.47.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vitest": "^0.2.8",
    "husky": "^8.0.3",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "ts-prune": "^0.10.3",
    "tsconfig-paths": "4.1.1",
    "typescript": "^5.1.6",
    "unplugin-swc": "^1.3.2",
    "vitest": "^0.32.4"
  }
}
