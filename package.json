{
  "name": "api-eclaire",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "18.16"
  },
  "packageManager": "yarn@4.8.1",
  "scripts": {
    "amibroken": "yarn dedupe --check && yarn typecheck && yarn lint && yarn test",
    "bash:production": "scalingo -a eclaire-api run bash",
    "build": "nest build",
    "deadcode": "ts-prune --ignore 'vitest.config.ts'",
    "depcheck": "depcheck",
    "dev": "nest start --watch",
    "etl:create-index": "ts-node src/etl/console.ts create-index",
    "etl:create-policies": "ts-node src/etl/console.ts create-policies",
    "etl:daily-update:everything": "ts-node src/etl/console.ts daily-update 1970-01-01",
    "etl:daily-update:yesterday": "ts-node src/etl/console.ts daily-update",
    "etl:delete-pipelines": "ts-node src/etl/console.ts delete-pipelines",
    "etl:delete-policies": "ts-node src/etl/console.ts delete-policies",
    "etl:import": "ts-node src/etl/console.ts import",
    "etl:hard-index-migration": "ts-node src/etl/console.ts hard-index-migration",
    "etl:meddra-import": "ts-node src/etl/console.ts meddra-import",
    "etl:reset-index": "ts-node src/etl/console.ts reset-index",
    "etl:translate:everything": "ts-node src/etl/console.ts translate 1970-01-01",
    "etl:translate:yesterday": "ts-node src/etl/console.ts translate",
    "etl:update-meddra-labels:everything": "ts-node src/etl/console.ts update-meddra-labels 1970-01-01",
    "etl:update-meddra-labels:yesterday": "ts-node src/etl/console.ts update-meddra-labels",
    "lint": "eslint 'src/**/*.ts' --max-warnings=0",
    "prepare": "husky install",
    "start": "node dist/src/main",
    "start:db": "yarn stop:db && podman compose up -d",
    "start:debug": "nest start --debug --watch",
    "stop:db": "podman compose down",
    "test": "NODE_ENV=test vitest run",
    "test:coverage": "yarn test --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:mutation:api": "npx stryker run stryker-api.conf.json",
    "test:mutation:etl": "npx stryker run stryker-etl.conf.json",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@elastic/elasticsearch": "7.17.14",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^6.4.0",
    "@sentry/nestjs": "^9.11.0",
    "@types/fhir": "^0.0.41",
    "express": "^4.21.2",
    "node-fetch": "2.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.15",
    "@stryker-mutator/core": "^7.3.0",
    "@stryker-mutator/typescript-checker": "^7.3.0",
    "@stryker-mutator/vitest-runner": "^7.3.0",
    "@swc/core": "^1.11.9",
    "@types/express": "^4.17.21",
    "@types/node": "18.19.80",
    "@types/node-fetch": "^2.6.12",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitest/coverage-istanbul": "^0.29.8",
    "depcheck": "^1.4.7",
    "eslint": "^8.47.0",
    "eslint-import-resolver-typescript": "^4.3.1",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vitest": "^0.2.8",
    "husky": "^8.0.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.1.3",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.6",
    "unplugin-swc": "^1.5.1",
    "vitest": "^0.32.4"
  }
}
