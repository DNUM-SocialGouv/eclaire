{
  "name": "api-eclaire",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "18.15"
  },
  "packageManager": "yarn@3.5.0",
  "scripts": {
    "amibroken": "yarn dedupe --check && yarn typecheck && yarn lint && yarn test && yarn test:e2e",
    "bash:production": "scalingo -a eclaire-api run bash",
    "build": "nest build",
    "deadcode": "ts-prune --ignore 'vitest.config.ts'",
    "depcheck": "depcheck",
    "dev": "nest start --watch",
    "etl:create-index": "ts-node src/etl/console.ts create-index",
    "etl:import": "ts-node src/etl/console.ts import-riph",
    "lint": "eslint '{src,config}/**/*.ts' --max-warnings=0",
    "start": "node dist/src/main",
    "start:debug": "nest start --debug --watch",
    "start:db": "yarn stop:db && docker-compose up -d",
    "stop:db": "docker-compose down",
    "test": "NODE_ENV=test vitest --run",
    "test:coverage": "yarn test --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:mutation": "npx stryker run",
    "transpile": "tsc -p tsconfig.mutation.json",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.10.0",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.2.1",
    "@sentry/minimal": "^6.19.7",
    "@sentry/node": "^7.38.0",
    "@sentry/tracing": "^7.38.0",
    "argon2": "^0.30.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "js-yaml": "^4.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@stryker-mutator/core": "^6.4.2",
    "@stryker-mutator/jest-runner": "^6.4.2",
    "@stryker-mutator/typescript-checker": "^6.4.2",
    "@types/express": "^4.17.17",
    "@types/jest": "29.2.4",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "18.11.18",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitest/coverage-istanbul": "^0.29.7",
    "depcheck": "^1.4.3",
    "eslint": "^8.36.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "29.3.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-prune": "^0.10.3",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.9.5",
    "vitest": "^0.29.7"
  }
}
